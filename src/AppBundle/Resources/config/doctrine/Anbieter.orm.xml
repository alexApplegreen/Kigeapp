<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Anbieter" table="anbieter">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="beschreibung" type="string" column="beschreibung" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="homepageUrl" type="string" column="homepage_url" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rating" type="integer" column="rating" nullable="true">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="sum" type="integer" column="sum" nullable="true">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="gesamt" type="integer" column="gesamt" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="profilbildId" type="integer" column="profilbild_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
